[submodule "bash/.bash_it"]
	path = bash/dot-bash_it
	url = git@github.com:Bash-it/bash-it.git
[submodule "oh-my-zsh/.oh-my-zsh"]
	path = zsh/dot-oh-my-zsh
	url = git@github.com:nbrown/oh-my-zsh.git
[submodule "vim/.spf13-vim-3"]
	path = vim/dot-spf13-vim-3
	url = git@github.com:spf13/spf13-vim.git
[submodule "vim/.vim"]
	path = vim/dot-vim
	url = git@github.com:nbrown/vim-files
	ignore = dirty
